meta {
  name: Get User With GraphQL
  type: graphql
  seq: 1
}

post {
  url: http://localhost:9090/api/graphql
  body: graphql
  auth: none
}

body:graphql {
  query {
    userById(id: "f5e4e2a8-7f07-4b2e-9b7c-6c6b1fcd3c42") {
      phoneNumbers
      username
      email
      role {
        code
        name
      }
    }
  }
  
  #No role because JDBC cant save Role directly
}
